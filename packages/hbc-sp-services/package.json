{
  "name": "@hbc/sp-services",
  "version": "1.1.0",
  "private": false,
  "description": "Shared SPFx-compatible data/services/models package for HBC Suite applications.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "README.md",
    "package.json"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf lib",
    "prepack": "npm run clean && npm run build",
    "pack:local": "npm pack --dry-run",
    "publish:check": "npm publish --dry-run --access restricted",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --ci",
    "test:mutation": "stryker run"
  },
  "dependencies": {
    "@microsoft/applicationinsights-web": "^3.3.11"
  },
  "peerDependencies": {
    "@fluentui/react-icons": "^2.0.230",
    "@microsoft/signalr": "^8.0.0 || ^10.0.0",
    "@pnp/graph": "^4.4.1",
    "@pnp/logging": "^4.4.1",
    "@pnp/queryable": "^4.4.1",
    "@pnp/sp": "^4.4.1",
    "react": ">=18.2.0 <19",
    "react-dom": ">=18.2.0 <19"
  },
  "peerDependenciesMeta": {
    "@fluentui/react-icons": {
      "optional": true
    }
  },
  "devDependencies": {
    "@fluentui/react-icons": "2.0.319",
    "@stryker-mutator/core": "^9.5.1",
    "@stryker-mutator/jest-runner": "^9.5.1",
    "@stryker-mutator/typescript-checker": "^9.5.1",
    "@types/jest": "^29.5.12",
    "@xmldom/xmldom": "^0.8.11",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "~5.3.3"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
